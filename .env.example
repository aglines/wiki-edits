# Google Cloud Platform Configuration
GCP_PROJECT_ID=your-project-id
GCP_REGION=your-region

# Pub/Sub Configuration
PUBSUB_TOPIC=projects/your-project-id/topics/your-topic
PUBSUB_SUBSCRIPTION=projects/your-project-id/subscriptions/your-sub

# Google Cloud Storage
GCS_TEMP_LOCATION=gs://your-bucket-name/temp

# BigQuery Configuration
BIGQUERY_OUTPUT_TABLE=your-project-id.your_dataset.your-table

# Dataflow Configuration
DATAFLOW_MAX_WORKERS=15
DATAFLOW_NUM_WORKERS=2
DATAFLOW_MACHINE_TYPE=n1-standard-2
DATAFLOW_DISK_SIZE_GB=50

# Logging
LOG_LEVEL=INFO
